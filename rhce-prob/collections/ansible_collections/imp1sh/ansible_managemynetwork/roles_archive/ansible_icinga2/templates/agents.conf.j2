/** Generated by Ansible */
{% for icingahost in icinga2_hosts %}
{% if icingahost['agent'] %}
object Endpoint "{{ icingahost['name'] }}" {
  host = "{{ icingahost['name'] }}"
}
object Zone "{{ icingahost['name'] }}" {
  endpoints = [ "{{ icingahost['name'] }}" ]
  parent = "{{ icinga2_conf_zonename }}"
}

{% endif %}
{% endfor %}
