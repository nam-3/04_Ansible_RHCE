/** Generated by Ansible */
{% for user in icingausers %}
object User "{{ user['name'] }}" {
  display_name = "{{ user['fullname'] }}"
  enable_notifications = {% if user['enablenotify'] %}true{% else %}false{% endif %}
  states = [ {{ user['notifystates']|join(', ') }} ]
  types = [ {{ user['notifytypes']|join(', ') }} ]
  email = {{ user['email'] }}
}

{% endfor %}
